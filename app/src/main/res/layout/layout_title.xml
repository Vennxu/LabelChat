<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/title_relative"
              android:layout_width="match_parent"
              android:layout_height="50dp"
              android:background="@color/lcPrimaryColor"

    >
    <ImageView
        android:id="@+id/title_image_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:background="@drawable/lc_ic_ab_back_am"
        />
    <TextView
        android:id="@+id/title_text_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/get_get"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/title_image_back"
        style="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
        />

</RelativeLayout>
